{
  "expo": {
    "jsEngine": "hermes",
    "name": "thrive-mobile",
    "owner": "michaeljoyce",
    "slug": "thrive-mobile-private",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "entryPoint": "./src/App.tsx",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaeljoyce.thrivemobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.michaeljoyce.thrivemobile"
    },
    "extra": {
      "eas": {
        "projectId": "1264c061-6a78-48b7-87e4-a4661c994b5f"
      }
    }
  }
}
